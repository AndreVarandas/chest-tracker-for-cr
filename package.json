{
  "name": "chest-tracker-cr",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "commit": "git-cz",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --ignore-path .gitignore",
    "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --cache --ext .js,.jsx,.ts,.tsx --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "expo": "~46.0.16",
    "expo-font": "~10.2.0",
    "expo-localization": "~13.1.0",
    "expo-status-bar": "~1.4.0",
    "i18n-js": "^4.1.1",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.6",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/react": "~18.0.14",
    "@types/react-native": "~0.69.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "~4.3.5"
  },
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-emoji"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": "npm run lint:fix",
    "*.{js,ts,tsx,jsx,html,css,md}": "npm run format"
  }
}
